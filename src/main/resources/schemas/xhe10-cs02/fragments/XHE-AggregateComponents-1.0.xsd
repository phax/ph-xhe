<xsd:schema xmlns="oasis-cefact-xhe-1.0-AggregateComponents"
            xmlns:xha="oasis-cefact-xhe-1.0-AggregateComponents"
            xmlns:xhb="oasis-cefact-xhe-1.0-BasicComponents"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="oasis-cefact-xhe-1.0-AggregateComponents"
            elementFormDefault="qualified"
            attributeFormDefault="unqualified"
            version="1.1">
   <xsd:import namespace="oasis-cefact-xhe-1.0-BasicComponents"
               schemaLocation="XHE-BasicComponents-1.0.xsd"/>
    <!-- ==== Incorporate any desired constraints on the payload -->
    <xsd:include schemaLocation="XHE-PayloadContentDataType-1.0.xsd"/>
        <xsd:element name="BusinessScope" type="BusinessScopeType"/>
   <xsd:element name="BusinessScopeCriterion" type="BusinessScopeCriterionType"/>
   <xsd:element name="ExternalReference" type="ExternalReferenceType"/>
   <xsd:element name="FromParty" type="PartyType"/>
   <xsd:element name="Header" type="HeaderType"/>
   <xsd:element name="InstanceDecryptionInformationExternalReference"
                type="ExternalReferenceType"/>
   <xsd:element name="InstanceDecryptionKeyExternalReference"
                type="ExternalReferenceType"/>
   <xsd:element name="Party" type="PartyType"/>
   <xsd:element name="PartyIdentification" type="PartyIdentificationType"/>
   <xsd:element name="Payload" type="PayloadType"/>
   <xsd:element name="PayloadExternalReference" type="ExternalReferenceType"/>
   <xsd:element name="Payloads" type="PayloadsType"/>
   <xsd:element name="RelevantExternalReference" type="ExternalReferenceType"/>
   <xsd:element name="ToParty" type="PartyType"/>
   <xsd:complexType name="BusinessScopeType">
      <xsd:sequence>
         <xsd:element ref="xha:BusinessScopeCriterion"
                      minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="xha:ExternalReference" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="BusinessScopeCriterionType">
      <xsd:sequence>
         <xsd:element ref="xhb:BusinessScopeCriterionTypeCode"
                      minOccurs="1"
                      maxOccurs="1"/>
         <xsd:element ref="xhb:BusinessScopeCriterionValue" minOccurs="1" maxOccurs="1"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ExternalReferenceType">
      <xsd:sequence>
         <xsd:element ref="xhb:ID" minOccurs="1" maxOccurs="1"/>
         <xsd:element ref="xhb:AvailabilityStartDateTime" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:AvailabilityEndDateTime" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:Login" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:Password" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="HeaderType">
      <xsd:sequence>
         <xsd:element ref="xhb:ID" minOccurs="1" maxOccurs="1"/>
         <xsd:element ref="xhb:UUID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:CreationDateTime" minOccurs="1" maxOccurs="1"/>
         <xsd:element ref="xha:BusinessScope" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xha:FromParty" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xha:ToParty" minOccurs="1" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PartyType">
      <xsd:sequence>
         <xsd:element ref="xha:PartyIdentification" minOccurs="1" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PartyIdentificationType">
      <xsd:sequence>
         <xsd:element ref="xhb:ID" minOccurs="1" maxOccurs="1"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PayloadType">
      <xsd:sequence>
         <xsd:element ref="xhb:ID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:Description" minOccurs="0" maxOccurs="unbounded"/>
         <xsd:element ref="xhb:DocumentTypeCode" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:ContentTypeCode" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:CustomizationID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:ProfileID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:ProfileExecutionID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:HandlingServiceID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:ValidationTypeCode" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:ValidationVersionID" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:InstanceEncryptionIndicator" minOccurs="1" maxOccurs="1"/>
         <xsd:element ref="xhb:InstanceEncryptionMethod" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xhb:InstanceHashValue" minOccurs="0" maxOccurs="1"/>
         <xsd:element ref="xha:InstanceDecryptionInformationExternalReference"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element ref="xha:InstanceDecryptionKeyExternalReference"
                      minOccurs="0"
                      maxOccurs="1"/>
         <xsd:element ref="xha:RelevantExternalReference"
                      minOccurs="0"
                      maxOccurs="unbounded"/>
         <xsd:element ref="xha:PayloadExternalReference" minOccurs="0" maxOccurs="1"/>
         <xsd:element name="PayloadContent"
                      minOccurs="0"
                      maxOccurs="1"
                      type="PayloadContentType">
           <xsd:annotation>
               <xsd:documentation>The content of the payload when it is included in the envelope.</xsd:documentation>
           </xsd:annotation>
         </xsd:element>
        </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PayloadsType">
      <xsd:sequence>
         <xsd:element ref="xha:Payload" minOccurs="1" maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
</xsd:schema>
